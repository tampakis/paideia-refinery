    li.wym_tools_unlink a {
      background-image: image_url('wymeditor/skins/refinery/link_break.png');
    }
    li.wym_tools_image a {
      background-image: image_url('wymeditor/skins/refinery/photo_add.png');
    }
    li.wym_tools_table a {
      background-image: image_url('wymeditor/skins/refinery/table_add.png');
    }
    li.wym_tools_html a {
      background-image: image_url('wymeditor/skins/refinery/page_code.png');
    }
  }
}

