    float: left;
    padding: 0px;
    margin: 0px 12px 0px 0px;
    list-style: none;
    a {
      border-bottom: 0px none;
      img {
        border-width: 4px;
        border-style: solid;
        border-color: transparent;
        display: block;
      }
    }
