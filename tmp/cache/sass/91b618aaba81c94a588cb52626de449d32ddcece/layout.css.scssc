}
.form-actions.form-actions-dialog {
  @include rounded(0px);
}
#menu > a {
  @include top-rounded(5px);
}
.ui-tabs .ui-state-default a {
  @include top-rounded(5px);
}
.tooltip, .tooltip span {
  @include rounded(6px);
}

.label {
